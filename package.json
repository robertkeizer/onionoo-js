{
	"name": "onionoo-js",
	"version": "0.0.1",
	"private": false,
	"main": "lib/index.js",
	"scripts": {
		"test": "npm run jshint && npm run test-node",
		"jshint": "jshint -c .jshint lib/*.js test/*.js",
		"test-node": "istanbul test ./node_modules/mocha/bin/_mocha test/*.js -- -t 10000",
		"coverage": "npm test --coverage && istanbul check-coverage --lines 100 --function 100 --statements 100 --branches 100"
	},
	"devDependencies": {
		"istanbul": "^0.2.7",
		"jshint": "~2.3.0",
		"mocha": "~1.18",
		"assert": "1.3.0"
	},
	"dependencies": {
		"request": "2.58.0",
		"htmlescape2": "2.8.3",
		"async": "1.3.0",
		"elasticsearch": "5.0.0",
		"joi": "6.6.0"
	}
}
